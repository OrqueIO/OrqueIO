<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
	xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
	xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
	targetNamespace="Examples">
	
	<signal id="alertSignal" name="alert" />

	<process id="throwPayloadSignal" isExecutable="true">

		<startEvent id="start" />

		<sequenceFlow sourceRef="start" targetRef="end" />

		<endEvent id="end">
			<extensionElements>
				<orqueio:inputOutput>
					<orqueio:inputParameter name="localVar1">localVal1</orqueio:inputParameter>
					<orqueio:inputParameter name="localVar2">localVal2</orqueio:inputParameter>
				</orqueio:inputOutput>
			</extensionElements>
			<signalEventDefinition signalRef="alertSignal">
				<extensionElements>
					<orqueio:in variables="all" local="true" />
				</extensionElements>
			</signalEventDefinition>
		</endEvent>

	</process>

</definitions>
