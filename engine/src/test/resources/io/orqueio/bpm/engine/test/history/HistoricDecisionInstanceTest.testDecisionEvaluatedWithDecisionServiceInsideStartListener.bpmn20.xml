<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskAssigneeExample" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">
  
  <process id="testProcess" isExecutable="true">
  
    <startEvent id="start"/>
    
    <sequenceFlow sourceRef="start" targetRef="task" />

    <serviceTask id="task" orqueio:expression="${true}">
      <extensionElements>
        <orqueio:executionListener class="io.orqueio.bpm.engine.test.history.DecisionServiceDelegate" event="start" />
      </extensionElements>
    </serviceTask>

    <sequenceFlow sourceRef="task" targetRef="end" />
    
    <endEvent id="end" orqueio:asyncBefore="true" />

  </process>

</definitions>
