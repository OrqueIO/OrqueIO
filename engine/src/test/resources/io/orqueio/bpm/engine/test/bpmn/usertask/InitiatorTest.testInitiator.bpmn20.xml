<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskAssigneeExample" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">
  
  <process id="InitiatorProcess" isExecutable="true">
  
    <startEvent id="start" orqueio:initiator="initiator" />
    
    <sequenceFlow id="flow1" sourceRef="start" targetRef="theTask" />

    <userTask id="theTask" name="my task" orqueio:assignee="${initiator}" />
    
    <sequenceFlow id="flow2" sourceRef="theTask" targetRef="theEnd" />
    
    <endEvent id="theEnd" />
    
  </process>

</definitions>