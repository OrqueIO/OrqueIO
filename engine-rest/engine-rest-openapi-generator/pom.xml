  <name>Orque IO Platform - engine - REST - OpenAPI (JSON Generator)</name>
  <packaging>jar</packaging>
  <description>orqueio-engine-rest-openapi-generator</description>



  <parent>
    <groupId>io.orqueio.bpm</groupId>
    <artifactId>orqueio-engine-rest-root</artifactId>
