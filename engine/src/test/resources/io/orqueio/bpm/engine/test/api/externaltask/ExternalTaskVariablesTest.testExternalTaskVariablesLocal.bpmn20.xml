<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskAssigneeExample"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="oneExternalTaskProcess" isExecutable="true">

    <startEvent id="start"/>

    <sequenceFlow id="flow1" sourceRef="start" targetRef="externalTask" />

    <serviceTask id="externalTask" orqueio:type="external" orqueio:topic="testTopic">
      <extensionElements>
        <orqueio:inputOutput>
          <orqueio:inputParameter name="localVar">localVal</orqueio:inputParameter>
        </orqueio:inputOutput>
      </extensionElements>
    </serviceTask>

    <sequenceFlow id="flow2" sourceRef="externalTask" targetRef="end" />

    <endEvent id="end" />

  </process>

</definitions>