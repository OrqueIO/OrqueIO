<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">


  <process id="theProcess" isExecutable="true" orqueio:historyTimeToLive="5">

    <startEvent id="start" />

    <sequenceFlow sourceRef="start" targetRef="serviceTask" />

    <serviceTask id="serviceTask" orqueio:delegateExpression="${failingSLSB}" orqueio:async="true" />
    
    <sequenceFlow sourceRef="serviceTask" targetRef="end" />

    <endEvent id="end" />

  </process>
</definitions>
