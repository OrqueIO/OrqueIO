<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:orqueio="http://orqueio.org/schema/1.0/bpmn" xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL BPMN20.xsd" id="_KvS8sPBXEeOtYvXzz5ksgw" targetNamespace="http://orqueio.org/schema/1.0/bpmn">
  <process id="testProcess" isExecutable="true">
    <startEvent id="start" />
    <sequenceFlow sourceRef="start" targetRef="task" />
    <serviceTask id="task">
      <extensionElements>
        <orqueio:connector>
          <orqueio:connectorId>testConnector</orqueio:connectorId>
          <orqueio:inputOutput>
            <orqueio:inputParameter name="in">
              <orqueio:script scriptFormat="groovy">
                // test if execution is available
                y = execution.getVariable('x')
                y + y
              </orqueio:script>
            </orqueio:inputParameter>
            <orqueio:outputParameter name="out">
              <orqueio:script scriptFormat="groovy">
                // test if connector is available
                y = connector.getVariable('x')
                y + y + y
              </orqueio:script>
            </orqueio:outputParameter>
          </orqueio:inputOutput>
        </orqueio:connector>
      </extensionElements>
    </serviceTask>
    <sequenceFlow sourceRef="task" targetRef="wait" />
    <receiveTask id="wait" />
    <sequenceFlow sourceRef="wait" targetRef="end" />
    <endEvent id="end" />
  </process>
</definitions>
