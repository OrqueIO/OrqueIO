<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="testProcess" isExecutable="true">

    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="subProcess" />

    <subProcess id="subProcess">
      <startEvent id="innerStart" />
      <sequenceFlow sourceRef="innerStart" targetRef="theTask" />
      <userTask id="theTask" name="my task">
        <extensionElements>
          <orqueio:taskListener event="delete" class="io.orqueio.bpm.engine.test.bpmn.tasklistener.util.RecorderTaskListener"/>
        </extensionElements>
      </userTask>
    </subProcess>

    <sequenceFlow id="flow2" sourceRef="subProcess" targetRef="theEnd" />
    <endEvent id="theEnd" />

  </process>

</definitions>
