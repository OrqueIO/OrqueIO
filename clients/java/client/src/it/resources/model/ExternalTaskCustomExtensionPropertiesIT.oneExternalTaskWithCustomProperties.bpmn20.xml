<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="oneExternalTaskWithCustomProperties" name="External Task with String Topic Process" isExecutable="true">
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="externalTask" />
    <serviceTask id="externalTask" name="externalTask" orqueio:type="external" orqueio:topic="foo">
      <extensionElements>
        <orqueio:properties>
          <orqueio:property name="property1" value="value1" />
          <orqueio:property name="property2" value="value2" />
          <orqueio:property name="property3" value="value3" />
        </orqueio:properties>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow2" sourceRef="externalTask" targetRef="end" />
    <endEvent id="end" />
  </process>
</definitions> 