<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="renderedFormProcess" isExecutable="true">

    <startEvent id="theStart">
      <extensionElements>
        <orqueio:formData>
            <orqueio:formField id="startStringField" label="Start String Field" type="string" defaultValue="someString" />
        </orqueio:formData>
      </extensionElements>
    </startEvent>

    <sequenceFlow sourceRef="theStart" targetRef="task" />

    <userTask id="task">
      <extensionElements>
        <orqueio:formData>
            <orqueio:formField id="taskStringField" label="Task String Field" type="string" defaultValue="someString" />
        </orqueio:formData>
      </extensionElements>
    </userTask>

    <sequenceFlow sourceRef="task" targetRef="theEnd" />

    <endEvent id="theEnd" />

  </process>

</definitions>