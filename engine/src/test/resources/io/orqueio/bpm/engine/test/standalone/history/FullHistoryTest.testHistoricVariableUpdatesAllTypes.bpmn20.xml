<?xml version="1.0" encoding="UTF-8"?>

<definitions id="definitions" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">
  
  <process id="HistoricVariableUpdateProcess" isExecutable="true">
  
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="setVariables" />

    <serviceTask id="setVariables" orqueio:class="io.orqueio.bpm.engine.test.standalone.history.VariableSetter" />
    <sequenceFlow id="flow2" sourceRef="setVariables" targetRef="task" />
    
    <userTask id="task" name="Schedule meeting" orqueio:assignee="kermit" />
    <sequenceFlow id="flow3" sourceRef="task" targetRef="end" />
    
    <endEvent id="end" />
    
  </process>

</definitions>
