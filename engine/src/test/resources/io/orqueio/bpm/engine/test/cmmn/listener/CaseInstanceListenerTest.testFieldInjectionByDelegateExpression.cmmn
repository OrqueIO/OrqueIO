<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<definitions id="_7f0c94c0-2a22-445d-b4b7-4fd181e08248"
                  xmlns="http://www.omg.org/spec/CMMN/20151109/MODEL"
                  xmlns:orqueio="http://orqueio.io/schema/1.0/cmmn"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  targetNamespace="Examples">
  <case id="case" name="Case">

    <casePlanModel id="CasePlanModel_1">

      <extensionElements>
        <orqueio:caseExecutionListener event="create" delegateExpression="${myListener}">

          <orqueio:field name="greeter" stringValue="The Case" />
          <orqueio:field name="helloWorld">
            <orqueio:string>Hello World</orqueio:string>
          </orqueio:field>
          <orqueio:field name="prefix" expression="${'orqueio'.substring(0,3)}" />
          <orqueio:field name="suffix">
            <orqueio:expression>${'orqueio'.substring(3,7)}</orqueio:expression>
          </orqueio:field>

        </orqueio:caseExecutionListener>

      </extensionElements>

      <planItem id="PI_HumanTask_1" definitionRef="HumanTask_1" />
      <humanTask id="HumanTask_1" name="A HumanTask" />

    </casePlanModel>
  </case>

</definitions>