<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="io.orqueio.bpm.engine.test.enginge.test.api.runtime">

  <process id="process" isExecutable="true">

    <startEvent id="theStart" orqueio:asyncBefore="true" />

    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="theServiceTask" />

    <serviceTask id="theServiceTask" name="my task" orqueio:expression="${true}" />

    <sequenceFlow id="flow2" sourceRef="theServiceTask" targetRef="theEnd" />

    <endEvent id="theEnd" />

  </process>

</definitions>
