<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="process" isExecutable="true">

    <startEvent id="start" />

    <sequenceFlow id="flow1" sourceRef="start" targetRef="left">
      <conditionExpression xsi:type="tFormalExpression" language="groovy" orqueio:resource="deployment://io/orqueio/bpm/engine/test/bpmn/sequenceflow/condition-left.groovy" />
    </sequenceFlow>

    <sequenceFlow id="flow2" sourceRef="start" targetRef="right">
      <conditionExpression xsi:type="tFormalExpression" language="groovy" orqueio:resource="classpath://io/orqueio/bpm/engine/test/bpmn/sequenceflow/condition-right.groovy" />
    </sequenceFlow>

    <userTask id="left" name="task left" />

    <userTask id="right" name="task right" />

  </process>

</definitions>
