<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="process" isExecutable="true">

    <startEvent id="theStart"/>

    <sequenceFlow sourceRef="theStart" targetRef="task" />

    <userTask id="task">
      <extensionElements>
        <orqueio:formData>
            <orqueio:formField id="stringField" label="String Field" type="string" defaultValue="someString">
              <orqueio:validation>
                 <orqueio:constraint name="validator" config="io.orqueio.bpm.engine.test.api.authorization.service.MyFormFieldValidator" />
              </orqueio:validation>
            </orqueio:formField>
        </orqueio:formData>
      </extensionElements>
    </userTask>

    <sequenceFlow sourceRef="task" targetRef="theEnd" />

    <endEvent id="theEnd" />

  </process>

</definitions>