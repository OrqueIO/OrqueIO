<?xml version="1.0" encoding="UTF-8" ?>
<definitions id="definitions"
             targetNamespace="http://orqueio.io/schema/1.0/bpmn20"
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn">

  <process id="FormPropertyHandlingProcess" isExecutable="true">

    <startEvent id="start" />

    <sequenceFlow id="flow1" sourceRef="start" targetRef="task" />

    <userTask id="task">
      <extensionElements>
        <orqueio:formData>
          <orqueio:formField id="stringField" label="String Field"
            type="string" defaultValue="someString">
            <orqueio:validation>
              <orqueio:constraint name="readonly" />
            </orqueio:validation>
          </orqueio:formField>
        </orqueio:formData>
      </extensionElements>
    </userTask>

    <sequenceFlow id="flow2" sourceRef="task" targetRef="end" />

    <endEvent id="end" />

  </process>

</definitions>
    
