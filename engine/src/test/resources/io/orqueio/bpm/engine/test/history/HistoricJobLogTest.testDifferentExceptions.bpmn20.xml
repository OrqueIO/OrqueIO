<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="process" isExecutable="true">

    <startEvent id="theStart" />

    <sequenceFlow sourceRef="theStart" targetRef="serviceTask1" />

    <serviceTask id="serviceTask1" orqueio:asyncBefore="true" orqueio:class="io.orqueio.bpm.engine.test.history.FirstFailingDelegate" />

    <sequenceFlow sourceRef="serviceTask1" targetRef="serviceTask2" />

    <serviceTask id="serviceTask2" orqueio:class="io.orqueio.bpm.engine.test.history.SecondFailingDelegate" />

    <sequenceFlow sourceRef="serviceTask2" targetRef="theEnd" />

    <endEvent id="theEnd" />

  </process>
</definitions>
