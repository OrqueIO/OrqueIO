<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="asyncOutputMappingSubProcess" isExecutable="true">
  
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="subProcess" />
    
    <subProcess id="subProcess">
      <extensionElements>
        <orqueio:inputOutput>
          <orqueio:outputParameter name="subProcessOutputMappingExecuted">${true}</orqueio:outputParameter>
        </orqueio:inputOutput>
      </extensionElements>
      
      <startEvent id="subProcessStart" />
      <sequenceFlow id="flow2" sourceRef="subProcessStart" targetRef="task" />
      <userTask id="task" orqueio:asyncBefore="true">
        <extensionElements>
          <orqueio:inputOutput>
            <orqueio:outputParameter name="taskOutputMappingExecuted">${true}</orqueio:outputParameter>
          </orqueio:inputOutput>
        </extensionElements>
      </userTask>
      <sequenceFlow id="flow3" sourceRef="task" targetRef="subProcessEnd" />
      <endEvent id="subProcessEnd" />
    </subProcess>
    
    <sequenceFlow id="flow4" sourceRef="subProcess" targetRef="end" />
    <endEvent id="end" />
    
  </process>

</definitions>

