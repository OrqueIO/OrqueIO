<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definition" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="process" isExecutable="true">

    <startEvent id="theStart" />

    <sequenceFlow sourceRef="theStart" targetRef="theTask" />

    <userTask id="theTask">
      <extensionElements>
        <orqueio:inputOutput>
          <orqueio:inputParameter name="inputVariable">Hello World!</orqueio:inputParameter>
        </orqueio:inputOutput>
      </extensionElements>
    </userTask>

    <sequenceFlow sourceRef="theTask" targetRef="theEnd" />

    <endEvent id="theEnd" />

  </process>

</definitions>