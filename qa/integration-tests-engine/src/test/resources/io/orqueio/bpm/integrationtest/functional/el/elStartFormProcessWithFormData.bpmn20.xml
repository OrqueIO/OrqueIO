<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="elStartFormProcess" name="The One Process With Start Form Data" isExecutable="true" orqueio:historyTimeToLive="5">
    <startEvent id="theStart">
      <extensionElements>
        <orqueio:formData>
          <orqueio:formField id="someString" label="${resolveFormDataBean.getValue()}"
                             type="string" defaultValue="${resolveFormDataBean.getValue()}" />
        </orqueio:formData>
      </extensionElements>
    </startEvent>
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="userTask" />
    <userTask id="userTask" />
    <sequenceFlow id="flow2" sourceRef="userTask" targetRef="theEnd" />
    <endEvent id="theEnd" />
  </process>

</definitions>
