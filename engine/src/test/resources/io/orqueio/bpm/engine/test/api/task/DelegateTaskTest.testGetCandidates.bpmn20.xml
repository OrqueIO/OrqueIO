<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskListenerExample"
	xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
	targetNamespace="Examples">

	<process id="DelegateTaskTest.testGetCandidates" name="Task  Listener Example" isExecutable="true">

		<startEvent id="theStart" />

		<sequenceFlow id="flow1" sourceRef="theStart" targetRef="task1" />

		<userTask id="task1" orqueio:candidateUsers="kermit, gonzo"
			orqueio:candidateGroups="management, accountancy">
			<extensionElements>
				<orqueio:taskListener event="create"
					class="io.orqueio.bpm.engine.test.api.task.DelegateTaskTestTaskListener" />
			</extensionElements>
		</userTask>

		<sequenceFlow id="flow2" sourceRef="task1" targetRef="task2" />

		<userTask id="task2" />

		<sequenceFlow id="flow3" sourceRef="task2" targetRef="theEnd" />

		<endEvent id="theEnd" />

	</process>

</definitions>
