<definitions 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="process" isExecutable="true">

    <startEvent id="start" />

    <sequenceFlow sourceRef="start" targetRef="task1" />

    <userTask id="task1" />

    <sequenceFlow sourceRef="task1" targetRef="end1" />

    <endEvent id="end1" />

    <boundaryEvent id="timer1" attachedToRef="task1">
      <extensionElements>
        <orqueio:executionListener event="start" class="io.orqueio.bpm.engine.test.bpmn.executionlistener.RecorderExecutionListener">
          <orqueio:field name="parameter" stringValue="start boundary listener" />
        </orqueio:executionListener>
      </extensionElements>
      <timerEventDefinition>
        <timeDuration>PT5M</timeDuration>
      </timerEventDefinition>
    </boundaryEvent>

    <sequenceFlow sourceRef="timer1" targetRef="task2" />

    <userTask id="task2" />

    <sequenceFlow sourceRef="task2" targetRef="end2" />

    <endEvent id="end2" />

    <boundaryEvent id="timer2" attachedToRef="task2">
      <extensionElements>
        <orqueio:executionListener event="end" class="io.orqueio.bpm.engine.test.bpmn.executionlistener.RecorderExecutionListener">
          <orqueio:field name="parameter" stringValue="end boundary listener" />
        </orqueio:executionListener>
      </extensionElements>
      <timerEventDefinition>
        <timeDuration>PT5M</timeDuration>
      </timerEventDefinition>
    </boundaryEvent>

    <sequenceFlow sourceRef="timer2" targetRef="end3" />

    <endEvent id="end3" />

  </process>
</definitions>