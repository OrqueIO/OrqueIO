<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="oneExternalTaskProcess" isExecutable="true">
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="externalTask" />
    <businessRuleTask id="externalTask" orqueio:type="external" orqueio:topic="externalTaskTopic">
      <extensionElements>
        <orqueio:properties>
          <orqueio:property name="key1" value="val1" />
          <orqueio:property name="key2" value="val2" />
        </orqueio:properties>
      </extensionElements>
    </businessRuleTask>
    <sequenceFlow id="flow2" sourceRef="externalTask" targetRef="end" />
    <endEvent id="end" />
  </process>
</definitions>
