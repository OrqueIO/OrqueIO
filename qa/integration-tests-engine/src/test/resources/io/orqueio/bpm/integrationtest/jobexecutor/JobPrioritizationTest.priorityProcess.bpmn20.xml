<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="priorityProcess" orqueio:jobPriority="${priorityBean.getPriority()}" isExecutable="true" orqueio:historyTimeToLive="5">

    <startEvent id="start" />
    <sequenceFlow sourceRef="start" targetRef="serviceTask" />
    <serviceTask id="serviceTask" orqueio:expression="${true}" orqueio:asyncBefore="true" />
    <sequenceFlow id="flow1" sourceRef="serviceTask" targetRef="end" />
    <endEvent id="end" />

  </process>
</definitions>
