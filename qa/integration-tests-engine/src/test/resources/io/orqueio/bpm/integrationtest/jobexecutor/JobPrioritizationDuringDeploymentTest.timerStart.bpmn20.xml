<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">


  <process id="timerStartProcess" isExecutable="true"  orqueio:historyTimeToLive="5">

    <startEvent id="start" orqueio:jobPriority="${priorityBean.getPriority()}"  >
      <timerEventDefinition>
        <timeCycle>R3/PT10H</timeCycle>
      </timerEventDefinition>
    </startEvent>
    <sequenceFlow sourceRef="start" targetRef="end" />
    <endEvent id="end" />

  </process>
</definitions>
