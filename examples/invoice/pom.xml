  <version>0.25.0-SNAPSHOT</version>
  <packaging>war</packaging>
  <description>orqueio-example-invoice</description>

  <name>Orque IO Platform - Example - Invoice</name>

  <parent>
    <groupId>io.orqueio.bpm.example</groupId>
    <artifactId>orqueio-example-root</artifactId>
    <relativePath>../pom.xml</relativePath>
    <version>0.25.0-SNAPSHOT</version>
