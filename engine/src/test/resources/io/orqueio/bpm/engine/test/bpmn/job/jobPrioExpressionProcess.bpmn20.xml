<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="jobPrioExpressionProcess" isExecutable="true">
  
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="task1" />
    <manualTask id="task1" orqueio:asyncBefore="true" orqueio:asyncAfter="true" orqueio:jobPriority="${priority}"/>    
    <sequenceFlow id="flow2" sourceRef="task1" targetRef="task2" />
    <manualTask id="task2" orqueio:asyncBefore="true" orqueio:asyncAfter="true" orqueio:jobPriority="${15}" />    
    <sequenceFlow id="flow3" sourceRef="task2" targetRef="task3" />
    <manualTask id="task3" orqueio:asyncBefore="true" orqueio:asyncAfter="true" orqueio:jobPriority="${execution.getVariable('priority')}" />    
    <sequenceFlow id="flow4" sourceRef="task3" targetRef="task4" />
    <manualTask id="task4" orqueio:asyncBefore="true" orqueio:asyncAfter="true" orqueio:jobPriority="#{16}" />    
    <sequenceFlow id="flow5" sourceRef="task4" targetRef="end" />
    <endEvent id="end" />
    
  </process>

</definitions>
