<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="oneExternalTaskWithExpressionTopicProcess" name="External Task with Expression Process Topic" isExecutable="true">
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="externalTask" />
    <serviceTask id="externalTask" name="externalTask" orqueio:type="external" orqueio:topic="${topicName}" />
    <sequenceFlow id="flow2" sourceRef="externalTask" targetRef="end" />
    <endEvent id="end" />
  </process>
</definitions>
