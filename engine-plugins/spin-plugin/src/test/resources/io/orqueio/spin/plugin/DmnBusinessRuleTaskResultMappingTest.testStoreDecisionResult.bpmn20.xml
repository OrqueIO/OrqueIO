<?xml version="1.0" encoding="UTF-8"?>
<definitions id="taskAssigneeExample"
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="testProcess" isExecutable="true">

    <startEvent id="start" />

    <sequenceFlow sourceRef="start" targetRef="ruleTask" />

    <businessRuleTask id="ruleTask"
      orqueio:decisionRef="testDecision">
      <extensionElements>
        <orqueio:inputOutput>
          <orqueio:outputParameter name="result">${decisionResult}</orqueio:outputParameter>
        </orqueio:inputOutput>
      </extensionElements>
    </businessRuleTask>

    <sequenceFlow sourceRef="ruleTask" targetRef="end" />

    <endEvent id="end" orqueio:asyncBefore="true" />

  </process>

</definitions>
