<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:orqueio="http://orqueio.io/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="testProcess" isExecutable="true">

    <startEvent id="theStart" />

    <sequenceFlow id="flow1"  sourceRef="theStart" targetRef="service" />

    <serviceTask id="service" orqueio:expression="${true}" orqueio:asyncAfter="true">

    </serviceTask>

    <!-- outgoing flow without a transition id -->
    <sequenceFlow sourceRef="service" targetRef="theEnd" />

    <endEvent id="theEnd" />

  </process>

</definitions>
